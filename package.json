{
  "name": "@aeaton/react-spotify",
  "version": "0.2.3",
  "files": [
    "dist"
  ],
  "main": "dist/cjs",
  "module": "dist/es",
  "types": "dist/types",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:**",
    "build:es": "tsc --outDir dist/es --declarationDir dist/types --declaration",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prewatch": "rimraf dist",
    "watch": "npm-run-all --parallel 'build:** --watch'",
    "typecheck": "tsc -noEmit",
    "lint": "eslint 'src/**/*'",
    "preversion": "npm-run-all --parallel lint typecheck",
    "version": "yarn build"
  },
  "dependencies": {
    "jso": "^4.1.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.43",
    "@types/spotify-api": "^0.0.3",
    "@types/spotify-web-playback-sdk": "^0.1.7",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "axios": "^0.21.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.4",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-standard": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "axios": "^0.19.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80
  }
}
