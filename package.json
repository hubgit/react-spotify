{
  "name": "@aeaton/react-spotify",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "main": "dist/cjs",
  "module": "dist/es",
  "types": "dist/types",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:**",
    "build:es": "tsc --outDir dist/es --declarationDir dist/types --declaration",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prewatch": "rimraf dist",
    "watch": "npm-run-all --parallel 'build:** --watch'",
    "typecheck": "tsc -noEmit",
    "lint": "eslint 'src/**/*'",
    "preversion": "npm-run-all --parallel lint typecheck",
    "version": "yarn build"
  },
  "devDependencies": {
    "@types/react": "^16.8.19",
    "@types/spotify-web-playback-sdk": "^0.1.6",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "axios": "^0.19.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.1"
  },
  "peerDependencies": {
    "axios": "^0.19.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
